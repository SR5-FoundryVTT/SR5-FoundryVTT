{{> 'systems/shadowrun5e/dist/templates/rolls/success-test-message.hbs'}}
<div class="sr5 chat-card roll-card">
    <div class="card-content">
        <div class="left-side">
            {{> 'systems/shadowrun5e/dist/templates/rolls/parts/Damage.hbs'
            name=(localize "SR5.IncomingDrain")
            damage=test.data.incomingDrain
            showAP=true
            }}
        </div>
        <div class="right-side">
            {{> 'systems/shadowrun5e/dist/templates/rolls/parts/Damage.hbs'
            name=(localize "SR5.ModifiedDrain")
            damage=test.data.modifiedDrain
            showAP=true
            }}
        </div>
    </div>
</div>