{{> 'systems/shadowrun5e/dist/templates/common/List/ListHeader.hbs'
        name=(concatStrings (localize 'SR5.Programs') " " program_count)
        itemId='program'
        icons=(ItemHeaderIcons 'program')
        rightSide=(ItemHeaderRightSide 'program')
}}
{{#each itemType.program as |item iid|}}
    {{> 'systems/shadowrun5e/dist/templates/common/List/ListItem.hbs'
            img=item.img
            name=item.name
            itemId=item._id
            icons=(ItemIcons item)
            rightSide=(ItemRightSide item)
            hasDesc="true"
            hasRoll="true"
            description=item.description.value
    }}
{{/each}}
