<form>
    <div class="dialog-content-common">
        {{> 'systems/shadowrun5e/dist/templates/apps/dialogs/parts/success-test-documents.hbs'}}
        <hr />
        <div>
            <div class="form-group">
                <label data-tooltip="SR5.Tooltips.CompilationTest.SpriteType">{{localize 'SR5.Labels.Action.SpriteType'}}</label>
                <select name="test.data.spriteTypeSelected" {{#if test.preparedActorUsed}}disabled data-tooltip="SR5.Tooltips.Item.DisabledForPreparedActor"{{/if}}>
                    {{#select test.data.spriteTypeSelected}}
                    {{#each config.spriteTypes as |name type|}}
                    <option value="{{type}}">
                        {{localize name}}
                    </option>
                    {{/each}}
                    {{/select}}
                </select>
            </div>
            <div class="form-group">
                <label data-tooltip="SR5.Tooltips.CompilationTest.Level">{{localize "SR5.Level"}}</label>
                <input name="test.data.level" type="number" class="disabled-for-display" value="{{test.data.level}}" {{#if test.preparedActorUsed}}disabled data-tooltip="SR5.Tooltips.Item.DisabledForPreparedActor"{{/if}}/>
            </div>
        </div>
    </div>
    <div>
        {{> 'systems/shadowrun5e/dist/templates/apps/dialogs/parts/success-test-common.hbs'}}
    </div>
</form>
