<nav class="tabs" data-group="tabs">
    {{#each tabs as |tab|}}
    <a class="{{#if tab.active}}active{{/if}}" data-group="tabs" data-tab="{{tab.id}}" data-action="tab">
        <i class="{{ tab.icon }}"></i> {{ localize (concatStrings "SR5.CompendiumBrowser.Tabs." tab.label) }}
    </a>
    {{/each}}
</nav>
