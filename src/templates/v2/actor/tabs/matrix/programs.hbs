<section class="tab hide-overflow{{#if tab.active}} active{{/if}}" data-group="{{tab.group}}" data-tab="{{tab.id}}">
    <div class="fill-container">
        {{> 'systems/shadowrun5e/dist/templates/v2/list-items/program/header.hbs'
            label='SR5.Programs'
            type='program' }}
        <div id="programs-scroll" class="scrollable">
        {{#each itemType.program}}
            {{> 'systems/shadowrun5e/dist/templates/v2/list-items/program/item.hbs' item=this }}
        {{/each}}
        </div>
    </div>
</section>
