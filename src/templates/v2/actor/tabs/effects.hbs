<section class="tab hide-overflow{{#if tab.active}} active{{/if}}" data-group="{{tab.group}}" data-tab="{{tab.id}}">
    <div class="fill-container">
        <div class="scrollable">
            {{> 'systems/shadowrun5e/dist/templates/v2/common/effect-list-header.hbs'
                label='SR5..Effects'
                tooltip='SR5.Tooltips.Effect.DocumentEffects' }}
            {{#each effects}}
            {{> 'systems/shadowrun5e/dist/templates/v2/common/effect-list-item.hbs' item=this }}
            {{/each}}
            <!-- Owned Item Effects. Actor only. -->
            {{#if itemEffects}}
            {{> 'systems/shadowrun5e/dist/templates/v2/common/effect-list-header.hbs'
                label='SR5.Labels.Sheet.ItemEffects'
                tooltip='SR5.Tooltips.Effect.ItemEffects'
                isItemEffect=true }}
            {{#each itemEffects}}
                {{> 'systems/shadowrun5e/dist/templates/v2/common/effect-list-item.hbs' item=this isItemEffect=true }}
            {{/each}}
            {{/if}}
        </div>
    </div>
</section>
